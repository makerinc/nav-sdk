    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.14",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild": "^0.25.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
